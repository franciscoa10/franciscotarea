       / *   public void CalculaAlgo ()
         {
             conta decimal = 0;
             foreach (elemento var en this.Detalle)
             {
                 conta = conta + (item.Cantidad * item.ProductoCarrito.Precio);
             }
             this.Sutotal = conta;
         } * /


